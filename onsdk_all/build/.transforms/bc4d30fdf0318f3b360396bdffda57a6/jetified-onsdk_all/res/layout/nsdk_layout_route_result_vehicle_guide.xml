<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.baidu.navisdk.ui.widget.EmptyTopLayout
        android:id="@+id/empty_top_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/empty_top_layout"
        android:layout_alignParentRight="true"
        android:layout_marginTop="74dp"
        android:src="@drawable/nsdk_drawable_route_result_intelli_drive_guide_up" />

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:adjustViewBounds="true"
        android:src="@drawable/nsdk_drawable_route_result_intelli_drive_guide_down"
        android:scaleType="fitXY" />

</RelativeLayout>