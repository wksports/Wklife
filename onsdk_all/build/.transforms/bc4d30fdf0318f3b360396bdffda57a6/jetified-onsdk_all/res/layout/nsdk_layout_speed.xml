<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    tools:background="#00ff00"
    android:layout_width="@dimen/navi_dimens_66dp"
    android:layout_height="@dimen/navi_dimens_66dp">


    <View
        android:id="@+id/bnav_rg_cp_cur_overspeed_anim_view"
        android:layout_width="@dimen/navi_dimens_60dp"
        android:layout_height="@dimen/navi_dimens_60dp"
        android:layout_centerInParent="true"
        android:background="@drawable/nsdk_drawable_rg_assist_overspeed_anim_bg" />

    <RelativeLayout
        android:id="@+id/bnav_rg_cp_car_speed_bg"
        android:layout_width="@dimen/navi_dimens_60dp"
        android:layout_height="@dimen/navi_dimens_60dp"
        android:layout_centerInParent="true"
        android:background="@drawable/nsdk_drawable_rg_assist_normal_speed"
        android:gravity="center">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:orientation="vertical">

            <TextView
                android:id="@+id/bnav_rg_cp_cur_car_speed"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:letterSpacing="-0.03"
                tools:text="0"
                android:textColor="#3884fc"
                android:textSize="@dimen/navi_dimens_28dp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/bnav_rg_cp_cur_car_speed_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="-5dp"
                android:text="km/h"
                android:textColor="#3884fc"
                android:textSize="@dimen/navi_dimens_10dp" />
        </LinearLayout>
    </RelativeLayout>
</RelativeLayout>