            android:layout_height="wrap_content">
        <!--  简易诱导 -->
        
        <!--  非顺行 -->
        <RelativeLayout
           android:id="@+id/rl_bnav_simle_not_along"
           android:layout_width="match_parent"
           android:layout_height="wrap_content"
           android:gravity="center"
           android:layout_marginTop="@dimen/navi_dimens_60dp"
           android:visibility="visible" >
           
            
           <ImageView
               android:id="@+id/bnav_rg_sg_turn_icon"
               android:layout_width="@dimen/navi_dimens_100dp"
               android:layout_height="@dimen/navi_dimens_100dp"
               android:layout_centerHorizontal="true"
               android:src="@drawable/nsdk_drawable_rg_ic_turn_front"
               android:visibility="visible" />
           <LinearLayout 
               android:id="@+id/ll_bnav_rg_after_meters"
               android:layout_height="wrap_content"
               android:layout_width="match_parent"
               android:layout_marginTop="@dimen/navi_dimens_10dp"
