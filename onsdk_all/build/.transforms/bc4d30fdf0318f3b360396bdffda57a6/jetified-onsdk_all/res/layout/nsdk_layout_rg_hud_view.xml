                    android:clickable="false"
		            android:ellipsize="marquee"
		            android:focusable="true"
		            android:focusableInTouchMode="true"
		            android:marqueeRepeatLimit="marquee_forever"
                    android:singleLine="true"
                    /><!-- android:layout_marginLeft="@dimen/nsdk_rg_text_margin" -->
                <TextView
                    android:id="@+id/bnav_rg_hw_after_meters_lable"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/nsdk_string_hw_after"
                    android:textSize="@dimen/nsdk_text_size_rg_hud_second"
                    android:textColor="@color/nsdk_rg_hud_second_info"
                    android:singleLine="true"
                    android:ellipsize="end"
                    android:textStyle="bold"
                    />
            </LinearLayout>
        
            <LinearLayout 
                android:id="@+id/bnav_rg_hw_ic_panel"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/bnav_rg_hw_after_meters_panel"
                android:layout_marginTop="@dimen/navi_dimens_9dp"
                android:orientation="horizontal"
                android:gravity="center_horizontal"
                >
             <!--    <TextView 
                    android:id="@+id/bnav_rg_hw_go_out_from_highway_word"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/nsdk_string_hw_go_out_from_highway"
                    android:textSize="@dimen/nsdk_text_size_rg_second_info"
                    android:textColor="@color/nsdk_rg_second_info"
                    android:singleLine="true"
                    android:ellipsize="end"
                    />
                     -->
          <!--      <TextView 
                    android:id="@+id/bnav_rg_hw_from_word"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/nsdk_string_hw_from"
                    android:textSize="@dimen/nsdk_text_size_rg_second_info"
                    android:textColor="@color/nsdk_rg_second_info"
                    android:singleLine="true"
                    android:ellipsize="end" />
                     --> 
                <TextView 
                    android:id="@+id/bnav_rg_hw_ic_code"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    tools:text="出口36"
                    android:textSize="28sp"
                    android:textColor="#000000"
                    android:background="#ffffff"
                    android:singleLine="true"
                    android:ellipsize="end"
                    android:textStyle="bold"
                    android:layout_marginRight="@dimen/navi_dimens_1dp"
                    android:paddingLeft="@dimen/navi_dimens_5dp"
                    android:paddingRight="@dimen/navi_dimens_5dp"
                    />
               <TextView 
                   android:id="@+id/bnav_rg_hw_go_to_word"
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:text="@string/nsdk_string_hw_go_to"
                   android:textSize="@dimen/nsdk_text_size_rg_hud_second"
                   android:textColor="@color/nsdk_rg_hud_second_info"
                   android:singleLine="true"
                   android:ellipsize="end"
                   android:textStyle="bold"
                   android:visibility="gone"/>
                <TextView 
                    android:id="@+id/bnav_rg_hw_enter_word"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/navi_dimens_4dp"
                    android:text="@string/nsdk_string_hw_enter"
                    android:textSize="@dimen/nsdk_text_size_rg_hud_second"
                    android:textColor="@color/nsdk_rg_hud_second_info"
                    android:singleLine="true"
                    android:textStyle="bold"
                    android:ellipsize="end"
                    />
                
            </LinearLayout>
        
            <LinearLayout 
                android:id="@+id/bnav_rg_hw_go_where_panel"
                android:layout_width="@dimen/navi_dimens_350dp"
                android:layout_height="wrap_content"
                android:layout_below="@id/bnav_rg_hw_ic_panel"
                android:layout_marginTop="@dimen/navi_dimens_9dp"
                android:layout_centerHorizontal="true"
                android:layout_gravity="center_horizontal"
                >
                <com.baidu.navisdk.ui.widget.AlwaysMarqueeTextView
                    android:id="@+id/bnav_rg_hw_go_where_multi_tv"
                    android:layout_width="@dimen/navi_dimens_350dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:clickable="false"
                    android:ellipsize="marquee"
                    android:focusable="true"
                    android:focusableInTouchMode="true"
                    android:marqueeRepeatLimit="marquee_forever"
                    android:singleLine="true"
                    tools:text="呼伦贝尔 "
                    android:textSize="@dimen/nsdk_text_size_rg_hud_second"
                    android:textColor="@color/nsdk_rg_main_info"
                    android:textStyle="bold"
                     />
            </LinearLayout>
        </RelativeLayout>
        </FrameLayout>
        <!-- 速度，剩余距离  -->
        <LinearLayout 
              android:id="@+id/ll_bnav_spedd_distance_image"
              android:layout_width="match_parent"
              android:layout_height="wrap_content"
              android:layout_above="@+id/rl_bnav_rg_hud_about_arriving_time"
              android:layout_marginBottom="@dimen/navi_dimens_44dp"
              android:layout_centerHorizontal="true"
              android:gravity="center" >
               
            <!-- 当前时速 -->
             <RelativeLayout 
                 android:layout_width="wrap_content"
                 android:layout_height="wrap_content"
                 android:layout_gravity="bottom"
                 android:layout_marginRight="@dimen/navi_dimens_50dp"
                  >
                   <FrameLayout 
                       android:id="@+id/fl_bnav_speed_layout"
                       android:layout_width="@dimen/navi_dimens_100dp"
                       android:layout_height="@dimen/navi_dimens_100dp" >
                    <RelativeLayout 
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">
                        
                     <com.baidu.navisdk.ui.routeguide.subview.widget.CircleProgressImageView
                          android:id="@+id/bnav_rg_cur_car_speed_progress"
                          android:layout_width="@dimen/navi_dimens_100dp"
                          android:layout_height="@dimen/navi_dimens_100dp"
                         
                          android:text="速度"    
                          android:textColor="#ff000000"
                          android:textSize="18sp"
                          android:textStyle="bold"
                           roundProgress:nsdk_bn_fill="false"
                           roundProgress:nsdk_bn_max="100"
                           roundProgress:nsdk_bn_progress_paint_width="@dimen/navi_dimens_4dp"
                            roundProgress:nsdk_bn_paint_color="#D1D1D1"
                           roundProgress:nsdk_bn_paint_width="@dimen/navi_dimens_1dp"
                             roundProgress:nsdk_bn_sub_progress_paint_color="#7F1A1A1A"
                           roundProgress:nsdk_bn_circle_background_color="#CC1A1A1A"
                           roundProgress:nsdk_bn_circle_fill="true"
                           roundProgress:nsdk_bn_main_progress_cap_round="true"/>
                            <LinearLayout 
                               android:layout_width="wrap_content"
                               android:layout_height="wrap_content"
                               android:layout_centerHorizontal="true"
                               android:layout_centerVertical="true"
                               android:orientation="vertical"
                               android:gravity="center">
                        <TextView 
                           android:id="@+id/bnav_rg_current_speed"
                           android:layout_width="wrap_content"
                           android:layout_height="wrap_content"
                           android:textSize="@dimen/nsdk_text_size_progress_circle_normal"
                           android:textColor="@color/nsdk_rg_main_info"
                           tools:text="98"
                           android:textStyle="bold"
                           android:layout_gravity="bottom"/>
                       <TextView 
                           android:layout_width="wrap_content"
                           android:layout_height="wrap_content"
                           android:textSize="10sp"
                           android:layout_marginTop="-4dp"
                           android:textColor="@color/nsdk_rg_hud_second_info"
                           android:text="km/h"
                           android:textStyle="bold"/>
                       </LinearLayout>
                      </RelativeLayout>
                  </FrameLayout>
                  <TextView 
                      android:layout_width="wrap_content"
                      android:layout_height="wrap_content"
                      android:text="当前时速"
                      android:layout_below="@id/fl_bnav_speed_layout"
                      android:layout_marginTop="@dimen/navi_dimens_12dp"
                      android:layout_centerHorizontal="true"
                      android:textSize="16sp"
                      android:textStyle="bold"
                      android:textColor="@color/nsdk_rg_hud_second_info"/>
            </RelativeLayout>
               
             <!-- 剩余距离 -->
                
                <RelativeLayout 
                     android:layout_width="wrap_content"
                     android:layout_height="wrap_content"
                     android:layout_gravity="bottom">
                       <FrameLayout 
                           android:id="@+id/fl_bnav_left_distance"
                           android:layout_width="@dimen/navi_dimens_100dp"
                           android:layout_height="@dimen/navi_dimens_100dp" >
                        <RelativeLayout 
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content">

                            <com.baidu.navisdk.ui.routeguide.subview.widget.CircleProgressImageView
                                android:id="@+id/bnav_rg_left_distance_progress"
                                android:layout_width="@dimen/navi_dimens_100dp"
                                android:layout_height="@dimen/navi_dimens_100dp"
                                android:layout_centerHorizontal="true"
                                android:layout_centerVertical="true"
                                android:text="剩余距离"
                                android:textColor="@color/nsdk_rg_assist_normal_speed"
                                android:textSize="18sp"
                                android:textStyle="bold"
                                roundProgress:nsdk_bn_circle_background_color="#CC1A1A1A"
                                roundProgress:nsdk_bn_circle_fill="true"
                                roundProgress:nsdk_bn_fill="false"
                                roundProgress:nsdk_bn_main_progress_cap_round="true"
                                roundProgress:nsdk_bn_max="100"
                                roundProgress:nsdk_bn_paint_color="#D1D1D1"
                                roundProgress:nsdk_bn_paint_width="@dimen/navi_dimens_1dp"
                                roundProgress:nsdk_bn_progress_paint_width="@dimen/navi_dimens_4dp"
                                roundProgress:nsdk_bn_sub_progress_paint_color="#7F1A1A1A" />
                              <LinearLayout 
                                   android:layout_width="wrap_content"
                                   android:layout_height="wrap_content"
                                   android:layout_centerHorizontal="true"
                                   android:layout_centerVertical="true"
                                   android:orientation="vertical"
                                   android:gravity="center">
                            <TextView 
                               android:id="@+id/bnav_rg_left_distance"
                               android:layout_width="wrap_content"
                               android:layout_height="wrap_content"
                               android:textSize="@dimen/nsdk_text_size_progress_circle_normal"
                               android:textStyle="bold"
                               android:textColor="@color/nsdk_rg_main_info"
                               tools:text="360"
                               />
                           <TextView 
                               android:layout_width="wrap_content"
                               android:layout_height="wrap_content"
                               android:layout_marginTop="-4dp"
                               android:textSize="10sp"
                               android:textColor="@color/nsdk_rg_hud_second_info"
                               android:textStyle="bold"
                               android:text="km"
                              />
                           </LinearLayout>
                          </RelativeLayout>
                      </FrameLayout>
                      <TextView 
                          android:layout_width="wrap_content"
                          android:layout_height="wrap_content"
                          android:text="剩余距离"
                          android:layout_below="@id/fl_bnav_left_distance"
                          android:layout_marginTop="@dimen/navi_dimens_12dp"
                          android:layout_centerHorizontal="true"
                          android:textStyle="bold"
                          android:textColor="@color/nsdk_rg_hud_second_info"
                          android:textSize="16sp"/>
                </RelativeLayout>
                           
           </LinearLayout>
        <RelativeLayout 
            android:id="@+id/rl_bnav_rg_hud_about_arriving_time"
            android:layout_width="match_parent"
            android:layout_height="@dimen/navi_dimens_22dp"
            android:layout_alignParentBottom="true"
            android:layout_marginBottom="@dimen/navi_dimens_8dp"
             >
             <TextView
                android:id="@+id/bnav_rg_about_reach_time"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_alignParentBottom="true"
                android:gravity="center"
                tools:text="预计15:40到达"
                android:textColor="@color/nsdk_rg_main_info"
                android:textSize="16sp"
                android:textStyle="bold" />
        
    </RelativeLayout>
    </RelativeLayout>
 
    <LinearLayout
        android:id="@+id/nav_hud_gps_status"
