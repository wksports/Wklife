             <ImageView
               android:id="@+id/bnav_rg_sg_along_icon"
               android:layout_width="@dimen/navi_dimens_100dp"
               android:layout_height="@dimen/navi_dimens_100dp"
               android:layout_centerHorizontal="true"
               android:src="@drawable/nsdk_drawable_rg_ic_turn_front"
               android:visibility="visible" />
            <LinearLayout
                android:id="@+id/ll_bnav_rg_along_meter"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_below="@id/bnav_rg_sg_along_icon"
                android:layout_marginTop="@dimen/navi_dimens_10dp"
                android:orientation="horizontal" >

                <TextView
                    android:id="@+id/bnav_rg_sg_along_word"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="@dimen/navi_dimens_1dp"
                    android:text="@string/nsdk_string_rg_along"
                    android:textColor="@color/nsdk_rg_hud_second_info"
                    android:textSize="@dimen/nsdk_text_size_rg_hud_second" />
                <com.baidu.navisdk.ui.widget.AlwaysMarqueeTextView
                    android:id="@+id/bnav_rg_sg_cur_road_name_tv" 
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:clickable="false"
                    android:ellipsize="marquee"
                    android:focusable="true"
                    android:focusableInTouchMode="true"
                    android:marqueeRepeatLimit="marquee_forever"
                    android:singleLine="true"
                    android:text="当前道路"
                    android:textColor="@color/nsdk_rg_main_info"
                    android:textSize="@dimen/nsdk_text_size_rg_hud_main"
                    android:textStyle="bold" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_below="@id/ll_bnav_rg_along_meter"
                android:layout_marginTop="@dimen/navi_dimens_9dp"
                android:orientation="horizontal" >
 
                <TextView
                    android:id="@+id/bnav_rg_sg_drive_word"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="@dimen/navi_dimens_1dp"
                    android:text="@string/nsdk_string_rg_driver"
                    android:textColor="@color/nsdk_rg_hud_second_info"
                    android:textSize="@dimen/nsdk_text_size_rg_hud_second" />
            
                <com.baidu.navisdk.ui.widget.AlwaysMarqueeTextView
                    android:id="@+id/bnav_rg_sg_cur_road_remain_dist_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:clickable="false"
                    android:ellipsize="marquee"
                    android:focusable="true"
                    android:focusableInTouchMode="true"
                    android:marqueeRepeatLimit="marquee_forever"
                    android:singleLine="true"
                    tools:text="12.5公里"
                    android:textColor="@color/nsdk_rg_main_info"
                    android:textSize="@dimen/nsdk_text_size_rg_hud_second"
                    android:textStyle="bold" />

            </LinearLayout>
        </RelativeLayout>
        
        <!--  高速模式  -->
        <RelativeLayout 
