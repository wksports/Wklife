                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/bnav_rg_hw_turn_icon"
                android:layout_marginTop="@dimen/navi_dimens_10dp"
                android:orientation="horizontal"
                android:gravity="center_horizontal"
                >
                <!-- TextView 
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/nsdk_string_hw_go_ahead"
                    android:textSize="@dimen/nsdk_text_size_rg_second_info"
                    android:textColor="@color/nsdk_rg_second_info"
                    android:singleLine="true"
                    android:ellipsize="end"
                    /-->
                <com.baidu.navisdk.ui.widget.AlwaysMarqueeTextView 
                    android:id="@+id/bnav_rg_hw_after_meters_info"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="@dimen/navi_dimens_1dp"
                    tools:text="57公里"
                    android:textSize="@dimen/nsdk_text_size_rg_hud_main"
