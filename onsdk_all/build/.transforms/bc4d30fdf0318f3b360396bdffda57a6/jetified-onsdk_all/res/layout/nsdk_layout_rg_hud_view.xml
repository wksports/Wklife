               android:layout_below="@id/bnav_rg_sg_turn_icon"
               android:layout_centerHorizontal="true">
            <TextView
                android:id="@+id/bnav_rg_sg_after_meters_info"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:ellipsize="end"
                android:layout_gravity="bottom"
                tools:text="384米"
                android:layout_marginRight="@dimen/navi_dimens_1dp"
                android:textColor="@color/nsdk_rg_main_info"
                android:textSize="@dimen/nsdk_text_size_rg_hud_main"
                android:textStyle="bold"
                android:visibility="visible" />

            <TextView
                android:id="@+id/bnav_rg_sg_after_label_info"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:text="@string/nsdk_string_hw_after"
                android:textColor="@color/nsdk_rg_hud_second_info"
                android:textSize="@dimen/nsdk_text_size_rg_hud_second"
                android:textStyle="bold"
                android:visibility="visible" />
           </LinearLayout>

           <LinearLayout
               android:id="@+id/ll_bna_rg_go_where"
               android:layout_width="match_parent"
               android:layout_height="wrap_content"
               android:layout_marginTop="@dimen/navi_dimens_9dp"
               android:layout_below="@id/ll_bnav_rg_after_meters"
               android:orientation="vertical"
               android:gravity="center" >

          <!--      <TextView
                   android:id="@+id/bnav_rg_sg_go_label_tv"
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:layout_marginRight="@dimen/nsdk_rg_text_margin"
                   android:paddingTop="@dimen/navi_dimens_8dp"
                   android:singleLine="true"
                   android:text="@string/nsdk_string_rg_come_in"
                   android:textColor="@color/nsdk_rg_second_info"
                   android:textSize="@dimen/nsdk_text_size_rg_second_info"
                   android:visibility="visible" />
--> 
               <com.baidu.navisdk.ui.widget.AlwaysMarqueeTextView
                   android:id="@+id/bnav_rg_sg_go_where_info"
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                    android:clickable="false"
                    android:ellipsize="marquee"
                    android:focusable="true"
                    android:focusableInTouchMode="true"
                    android:marqueeRepeatLimit="marquee_forever"
                    android:singleLine="true"
                   tools:text="上地信息路"
                   android:textColor="@color/nsdk_rg_main_info"
                   android:textSize="@dimen/nsdk_text_size_rg_hud_second"
                   android:textStyle="bold" />
           </LinearLayout>

       </RelativeLayout>
       <!--  顺行 -->
        <RelativeLayout
            android:id="@+id/bnav_rg_sg_along_road"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="@dimen/navi_dimens_60dp"
